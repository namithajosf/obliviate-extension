{
    "manifest_version": 3,
    "name": "Obliviate",
    "version": "1.0",
    "description": "A productivity tracker and focus tool.",
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
      }
    },
    "permissions": ["tabs", "storage", "alarms", "notifications"],
    "background": {
      "service_worker": "background.js"
    }
  }